{
	"name": "ollama-webui",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev --host --port 3000",
		"build": "vite build",
		"fmt": "npm run prettier:svelte && npm run prettier",
		"prettier:svelte": "npx -p prettier@2 -- prettier --plugin-search-dir . --write .",
