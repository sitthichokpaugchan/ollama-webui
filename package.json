		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/typography": "^0.5.10",
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/svelte": "^5.2.8",
		"@types/uuid": "^10.0.0",
		"@vitest/coverage-v8": "^3.2.4",
		"autoprefixer": "^10.4.16",
		"jsdom": "^26.1.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.0.0",
